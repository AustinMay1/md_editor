{
  "compilerOptions": {
      "target": "ES2015",
      "module": "commonjs",
      "lib": [ "ES2015", "dom"],
      "sourceMap": true,
      "outDir": "./scripts",
      "strict": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "noImplicitThis": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "esModuleInterop": true,
      "experimentalDecorators": true
  }
}
